<%= turbo_stream.replace "week_content_#{@week.id}" do %>
  <turbo-frame id="week_content_<%= @week.id %>">
    <%= render "public/jobs/table",
        week: @week,
        users: User.all,
        jobs: Job.where(week_id: @week.id).group_by(&:user_id),
        login_user: current_user %>
  </turbo-frame>
<% end %>
